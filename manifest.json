{
    "manifest_version": 3,
    "name": "Research Paper Searcher",
    "version": "1.0",
    "description": "Search and list relatable research papers",
    "permissions": [
      "activeTab"
    ],
    "host_permissions": [
      "https://api.semanticscholar.org/",
      "https://notegpt.io/pdf-summary"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
